{
  "name": "angular2-webpack",
  "version": "1.0.0",
  "description": "A webpack starter for angular 2",
  "scripts": {
    "start": "webpack-dev-server --inline --progress --port 3000 --config config/webpack.dev.js",
    "test": "karma start",
    "test-debug": "karma start --single-run=false --debug",
    "_build-doc" : "To build it do 'npm run build'",
    "build": "rimraf ../target/classes/webapp && webpack --config config/webpack.prod.js --progress --profile --bail"
  },
  "license": "MIT",
  "dependencies": {
    "@angular/common": "2.2.0",
    "@angular/compiler": "2.2.0",
    "@angular/core": "2.2.0",
    "@angular/forms": "2.2.0",
    "@angular/http": "2.2.0",
    "@angular/platform-browser": "2.2.0",
    "@angular/platform-browser-dynamic": "2.2.0",
    "@angular/router": "3.2.0",
    "@angular/upgrade": "2.2.0",
    "core-js": "2.4.1",
    "reflect-metadata": "0.1.8",
    "rxjs": "5.0.0-beta.12",
    "zone.js": "0.6.26"
  },
  "devDependencies": {
    "angular2-template-loader": "0.4.0",
    "css-loader": "0.23.1",
    "extract-text-webpack-plugin": "1.0.1",
    "file-loader": "0.8.5",
    "html-loader": "0.4.4",
    "html-webpack-plugin": "2.22.0",
    "jasmine-core": "2.5.1",
    "karma": "1.3.0",
    "karma-jasmine": "1.0.2",
    "karma-mocha-reporter": "2.1.0",
    "karma-phantomjs-launcher": "1.0.2",
    "karma-sourcemap-loader": "0.3.7",
    "karma-webpack": "1.7.0",
    "null-loader": "0.1.1",
    "phantomjs-prebuilt": "2.1.12",
    "raw-loader": "0.5.1",
    "rimraf": "2.5.4",
    "style-loader": "0.13.1",
    "ts-helpers": "1.1.1",
    "ts-loader": "0.8.2",
    "typescript": "2.0.3",
    "webpack": "1.13.2",
    "webpack-dev-server": "1.15.2",
    "webpack-merge": "0.14.1",

    "@types/core-js": "0.9.34",
    "@types/node": "6.0.48",
    "@types/jasmine": "2.5.38"
  }
}
